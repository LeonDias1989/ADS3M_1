package ordenacao.selection_sort;

public class SelectionSort {

	private int[] array = new int[10];
	
	 public int[] ordenarCrescente(){
		 
	        for (int fixo = 0; fixo < array.length; fixo++){
	 
	            for(int i = fixo + 1; i< array.length; i++){
	 
	                if(array[fixo] > array[i]){
	                    Long a = array[fixo];
	                    array[fixo] = array[i];
	                    array[i] = a;                     
	                }                            
	            }
	        }
	 
	        return array;
	 
	    }
	
}
