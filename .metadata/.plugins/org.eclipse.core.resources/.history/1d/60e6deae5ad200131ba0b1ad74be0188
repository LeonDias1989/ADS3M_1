package busca_binaria;

public class Contato<T> implements Comparable<Contato> {

	private T nome;
	private String telefone;

	public Contato(T nome, String telefone) {
		super();
		this.nome = nome;
		this.telefone = telefone;
	}

	public T getNome() {
		return nome;
	}

	public void setNome(T nome) {
		this.nome = nome;
	}

	public String getTelefone() {
		return telefone;
	}

	public void setTelefone(String telefone) {
		this.telefone = telefone;
	}

	@Override
	public int compareTo(Contato arg0) {

		return this.getNome().toString().compareTo(arg0.getNome().toString());
	}

}
